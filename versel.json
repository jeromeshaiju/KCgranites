{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manage.py"
      }
    ],
    "build": {
      "env": {
        "DJANGO_SETTINGS_MODULE": "your_project.settings"
      },
      "command": "python manage.py migrate && python manage.py collectstatic --noinput"
    }
  }
  